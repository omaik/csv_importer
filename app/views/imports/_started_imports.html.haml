%h4
  Started imports
%table.table
  %thead
    %tr
      %th Title
      %th Total
      %th Progress
      %th Errors
      %th Started
      %th
  %tbody
    - @started_imports.each do |import|
      %tr
        %td
          = import.title
        %td
          = import.total_count
        %td
          = import.progress
        %td
          = import.errors
        %td
          = import.started_at
        %td
          = link_to "Edit", edit_import_path(import), class: 'btn btn-success'


