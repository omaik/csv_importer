%h4
  Finished imports
%table.table
  %thead
    %tr
      %th Title
      %th Processed
      %th Errors
      %th Started
      %th Completed
      %th Actions
  %tbody
    - @finished_imports.each do |import|
      %tr
        %td
          = import.title
        %td
          = import.processed
        %td
          = import.errors_count
        %td
          = import.started_at
        %td
          = import.completed_at
        %td
          Delete

